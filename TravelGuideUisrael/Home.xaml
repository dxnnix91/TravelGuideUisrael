<?xml version="1.0" encoding="utf-8" ?>
<FlyoutPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="TravelGuideUisrael.Home"
             xmlns:pages="clr-namespace:TravelGuideUisrael" NavigationPage.HasBackButton="False">
  <FlyoutPage.Flyout>
    <pages:HomeFlyout x:Name="FlyoutPage" />
  </FlyoutPage.Flyout>
  <FlyoutPage.Detail>
      <NavigationPage>        
      <x:Arguments>
                <pages:HomeDetail/>
            </x:Arguments>
    </NavigationPage>
  </FlyoutPage.Detail>
</FlyoutPage>